{
  "name": "devhints",
  "displayName": "DevHints",
  "description": "View DevHints cheatsheets inside Visual Studio Code",
  "icon": "assets/icon.png",
  "version": "0.1.0",
  "publisher": "brpaz",
  "author": {
    "name": "Bruno Paz",
    "email": "oss@brunopaz.net",
    "url": "https://github.com/brpaz"
  },
  "repository": {
    "url": "https://github.com/brpaz/vsoce-devhints",
    "type": "git"
  },
  "license": "mit",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "devhints",
    "cheatsheets"
  ],
  "activationEvents": [
    "onCommand:extension.search",
    "onCommand:extension.clearCache"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.search",
        "title": "DevHints: Search Cheatsheets"
      },
      {
        "command": "extension.clearCache",
        "title": "DevHints: Clear cache"
      }
    ],
    "configuration": {
      "title": "DevHints settings",
      "properties": {
        "devhints.base_url": {
          "title": "Base url",
          "type": "string",
          "default": "https://devhints.io",
          "description": "The devhints base url. If you are using a fork, insert here your url."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "@types/node-fetch": "^2.1.4",
    "node-fetch": "^2.3.0"
  }
}
